---
import EventLayout from "@layouts/EventLayout.astro";
import type { ISpeaker } from "@utils/interfaces";

export const eventNickname = "EIYF 2023 August";

const allSpeakers = (await Astro.glob("../speakers/*.astro")) as ISpeaker[];

const speakers = allSpeakers.filter((speaker) =>
  speaker.events.includes("2023-08-08"),
);
---

<EventLayout
  eventName="Engineering In Your Future"
  eventNickname={eventNickname}
  eventDate="August 8th, 2023"
  speakers={speakers}
/>
