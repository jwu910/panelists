{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": ".",
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "paths": {
      "@assets/*": [
        "src/assets/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@speakers/*": [
        "src/speakers/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ]
    },
    "plugins": [
      {
        "name": "@astrojs/ts-plugin"
      }
    ],
    "strictNullChecks": true
  },
  "extends": "astro/tsconfigs/strict",
  "types": [
    "@astrojs/image/client"
  ]
}
